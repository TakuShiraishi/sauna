<% if user_signed_in? %>
    <%= form_with url: search_user_result_searches_path,method: :get do |f|%>
      <label><%= f.text_field :name, placeholder: "ユーザーの名前" %></label>
      <%= f.select 'method', options_for_select({ "完全一致" => "perfect", "部分一致" => "partial" }) %>
      <%= f.submit '検索' %>
    <% end %>
<% end %>