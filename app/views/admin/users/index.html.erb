<%= render 'layouts/header'%>
<div class="card text-center">
  <div class="card-header">
    ユーザー一覧
   </div>
    <div class="card-body">
      <div class="row">
    	<% @users.each do |user| %>
      <div class="col-md-12 col-12">
         <div class="card">
           <div class="card-header">
           <p class="card-text"><%= image_tag user.get_profile_image  %></p>
           <h5 class="card-title">名前:<%= user.name %></h5>
           <p class="card-text"><%= link_to "マイページ",admin_user_path(user.id) %></p>
          </div>
        </div>
     </div>
     <% end %>
  </div>
 </div>
</div>
<%= render 'layouts/footer'%>